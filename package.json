{
  "name": "extract-alphanumeric",
  "version": "1.0.0",
  "description": "simple tool to extract alphanumeric characters from a string",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js",
    "types": "./types/index.d.ts"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRobertLing/extract-alphanumeric.git"
  },
  "keywords": [
    "alphanumeric",
    "text-filter",
    "string-processing",
    "processing",
    "extract",
    "text"
  ],
  "author": "Robert Ling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheRobertLing/extract-alphanumeric/issues"
  },
  "homepage": "https://github.com/TheRobertLing/extract-alphanumeric#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  }
}
